#include <stack>
#include <cstdio>
#include <cstdlib>

using std::stack;
using std::printf;

int main() {
   stack<int> st = {};

   st.push(9);
   st.push(9998);
   st.push(5);
   st.push(5);
   st.push(9999);



   printf("result: %d\n", st.top());
    return 0;

    stack<int> evaluate(stack<int> st) {
       while (!st.empty()) {
           int x = st.top();
           st.pop();
           if (x == 9999) {
               if (st.size() < 2) {
                   printf("FAIL\n");
                   return st;
               }
               int y = st.top();
               st.pop();
               int z = st.top();
               st.pop();
               st.push(y + z);
           } else if (x == 9998) {
               if (st.size() < 2) {
                   printf("FAIL\n");
                   return st;
               }
               int y = st.top();
               st.pop();
               int z = st.top();
               st.pop();
               st.push(z - y);
           } else {
               // Push back the number if it's not an operator
               stack<int> temp;
               temp.push(x);
               while (!st.empty()) {
                   temp.push(st.top());
                   st.pop();
               }
               while (!temp.empty()) {
                   st.push(temp.top());
                   temp.pop();
               }
           }
       }
       return st;
    }
}
