#include<cstdio>
#include<vector>

using std::vector;
using std::printf;

void print_queue(const vector<int> q) {

for (size_t i= 0; i < q.size(); i++){
    printf("%d\n", q[i]);
}
    return ;

}

vector<int> push_q(vector<int> q, int e){
    q.push_back (e);
    return q;
}

int pop_q(vector<int> q){
    int r = q.at(1);        // save the popped element
    q.erase(q.begin());  // pop the element
    return r;             // returned popped element
}

int main() {
    vector<int> q = {};
   
    q = push_q(q, 10);    
    q = push_q(q, 20);
    q = push_q(q, 30);

    print_queue(q);

    int e = pop_q(q);
    printf("Popped %d\n", e);

    return 0;
}
